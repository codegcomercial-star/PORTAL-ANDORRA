{
  "name": "portal-andorra",
  "version": "0.1.0",
  "description": "Portal EconÃ³mico de Andorra - Comprehensive economic portal for Andorra",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "cd apps/web && npm run build",
    "start": "cd apps/web && npm run start",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "clean": "turbo run clean",
    "vercel-build": "cd apps/web && npm run build",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:migrate": "prisma migrate deploy --schema=prisma/schema.prisma",
    "db:seed": "tsx prisma/seeds/api-marketplace.ts",
    "db:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:push": "prisma db push --schema=prisma/schema.prisma",
    "db:generate": "prisma generate --schema=prisma/schema.prisma",
    "worker:dev": "cd apps/workers && pnpm dev",
    "prisma:studio": "npx prisma studio --schema=prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prepare": "husky install"
  },
